pkgname=asamos-autostart-cairo-dock-on-cinnamon
pkgver=0.1
pkgrel=1
pkgdesc="Aasamos / Autostart / Cairo Dock / On Cinnamon"
url="https://github.com/samwhelp/asamarch-packaging/tree/main/pack/core/de/cinnamon/asamos-autostart-cairo-dock-on-cinnamon"
arch=('any')
license=('MIT')
makedepends=()
depends=()
conflicts=()
provides=("${pkgname}")
options=(!strip !emptydirs)


prepare () {

	cp -af ../asset/. "${srcdir}"

}


package () {

	##
	## Main Config
	##

	cp -af "${srcdir}/overlay/." "${pkgdir}"

}
